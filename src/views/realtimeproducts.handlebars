<h1>Real Time Products</h1>
<h2>Agregar Productos en tiempo real</h2>
<h2>Formulario</h2>
<div class="container">
    <div class="formulario">
        <form id="addProduct" class="row g-3 col-md-8 col-lg-6">
            <label for="title">Titulo</label>
            <input type="text" name="title" id="title" required>
            <label for="description">Descripci√≥n</label>
            <input type="text" name="description" id="description" required>
            <label for="code">Codigo</label>
            <input type="text" name="code" id="code" required>
            <label for="price">Precio</label>
            <input type="text" name="price" id="price" pattern="[0-9]*" required>
            <label for="thumbnail">Imagen</label>
            <input type="text" name="thumbnail" id="thumbnail" required>
            <select id="status" class="form-select form-select-lg mb-3" aria-label="Large select example">
                <option value="true"> Activo </option>
                <option value="false"> Inactivo </option>
            </select>
            <label for="stock">Stock</label>
            <input type="text" name="stock" id="stock" pattern="[0-9]*" required>
            <label for="category">Categoria</label>
            <input type="text" name="category" id="category" required>
            <button type="submit" class="btn btn-light" id="btnEnviar">Submit</button>
        </form>
    </div>
    <div id="catalogue" class="d-flex flex-wrap flex-center">
    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
